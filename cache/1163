				"def A001163(n):",
				"    @cached_function",
				"    def h(k):",
				"        if k\u003c=0: return 1",
				"        S = sum((h(k-j)*h(j))/(j+1) for j in (1..k-1))",
				"        return (h(k-1)/k-S)/(1+1/(k+1))",
				"    return numerator(h(2*n)*2^n*rising_factorial(1/2,n))",
