				"(PARI)",
				"isStrongPsp(n,b)={",
				"        my(s,d,r,bm) ;",
				"        if( (n% 2) ==0 || n \u003c=1, return(0) ;) ;",
				"        if(isprime(n), return(0) ;) ;",
				"        s = valuation(n-1,2) ;",
				"        d = (n-1)/2^s ;",
				"        bm = Mod(b,n)^d ;",
				"        if ( bm == Mod(1,n), return(1) ;) ;",
				"        for(r=0,s-1,",
				"                bm = Mod(b,n)^d ;",
				"                if ( bm == Mod(-1,n),",
