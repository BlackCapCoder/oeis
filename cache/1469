				"(Sage) # Algorithm of L. Seidel (1877)",
				"# n -\u003e [a(1), ..., a(n)] for n \u003e= 1.",
				"def A001469_list(n) :",
				"    D = [0]*(n+2); D[1] = -1",
				"    R = []; b = False",
				"    for i in(0..2*n-1) :",
				"        h = i//2 + 1",
				"        if b :",
				"            for k in range(h-1, 0, -1) : D[k] -= D[k+1]",
				"        else :",
				"            for k in range(1, h+1, 1) :  D[k] -= D[k-1]",
				"        b = not b",
				"        if not b : R.append(D[h])",
				"    return R",
