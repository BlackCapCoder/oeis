			"program": [
				"(PARI) {a(n) = if(n\u003c3, 0, 1 + n^2 * (n-6) / 24 * prod(p=2, n, if( isprime(p) \u0026\u0026 (n%p==0), 1 - 1/p^2, 1)))}; /* _Michael Somos_, May 19 2004 */",
				"(PARI)",
				"a(n) = {",
				"  if (n \u003c 6, return(0));",
				"  my(f = factor(n), fsz = matsize(f)[1],",
				"     g = prod(k=1, fsz, f[k,1]),",
				"     h = prod(k=1, fsz, sqr(f[k,1]) - 1));",
				"  return(1 + (n-6)*sqr(n\\g)*h\\24);",
				"};",
