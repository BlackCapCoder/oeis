				"    local x,y ;",
				"    for x from 0 do",
				"        if 3*x^2 \u003e n then",
				"            return false;",
				"        end if;",
				"        for y from x do",
				"            if x^2+2*y^2 \u003e n then",
				"                break;",
				"            else",
				"                if issqr(n-x^2-y^2) then",
				"                    return true;",
				"                end if;",
				"            end if;",
