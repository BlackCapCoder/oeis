			"maple": [
				"A000928_list := proc(len)",
				"local ab, m, F, p, maxp; F := {};",
				"for m from 2 by 2 to len do",
				"   p := nextprime(m+1);",
				"   ab := abs(bernoulli(m));",
				"   maxp := min(ab, len);",
				"   while p \u003c= maxp do",
				"      if ab mod p = 0",
				"      then F := F union {p} fi;",
				"      p := nextprime(p);",
				"   od;",
				"od;",
				"sort(convert(F,list)) end:",
