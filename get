#!/bin/bash

if [ ! -f cache/$@ ]; then
  data=$(curl -s $(printf "https://oeis.org/search?fmt=json&q=id:A%06d" $@))
  echo "$data" > "cache/$@"
else
  data=$(cat "cache/$@")
fi


echo "$data" | jq -r '.results|.[0]|.name'
echo
echo "$data" | jq -r '.results|.[0]|.data'
echo
echo "$data" | jq -r '.results|.[0]|.program|.[]'
