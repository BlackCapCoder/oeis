				"a000959 n = a000959_list !! (n-1)",
				"a000959_list =  1 : sieve 2 [1,3..] where",
				"   sieve k xs = z : sieve (k + 1) (lucky xs) where",
				"      z = xs !! (k - 1 )",
				"      lucky ws = us ++ lucky vs where",
				"            (us, _:vs) = splitAt (z - 1) ws",
