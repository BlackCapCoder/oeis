				"    local ksu,k ;",
				"    option remember;",
				"    if n = 1 then",
				"        1;",
				"    elif n \u003c=3 then",
				"        2;",
				"    else",
				"        for k from 1 do",
				"            ksu := add(procname(i),i=1..k) ;",
				"            if n = ksu then",
				"                return (3+(-1)^k)/2 ;",
				"            elif n = ksu+ 1 then",
				"                return (3-(-1)^k)/2 ;",
				"            end if;",
