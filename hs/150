				"factBaseIntDs[n_] := Module[{m, i, len, dList, currDigit}, i = 1; While[n \u003e i!, i++ ]; m = n; len = i; dList = Table[0, {len}]; Do[ currDigit = 0; While[m \u003e= j!, m = m - j!; currDigit++ ]; dList[[len - j + 1]] = currDigit, {j, i, 1, -1}]; If[dList[[1]] == 0, dList = Drop[dList, 1]]; dList]; Table[FromDigits[factBaseIntDs[n]], {n, 0, 50}] (* _Alonso del Arte_, May 03 2006 *)",
