			"maple": [
				"A002145 := proc(n)",
				"    option remember;",
				"    if n = 1 then",
				"        3;",
				"    else",
				"        a := nextprime(procname(n-1)) ;",
				"        while a mod 4 \u003c\u003e  3 do",
				"            a := nextprime(a) ;",
				"        end do;",
				"        return a;",
				"    end if;",
				"end proc:",
