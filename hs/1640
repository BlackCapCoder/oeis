			"program": [
				"(Haskell)",
				"a035607 n k = a035607_tabl !! n !! k",
				"a035607_row n = a035607_tabl !! n",
				"a035607_tabl = map fst $ iterate",
				"   (\\(us, vs) -\u003e (vs, zipWith (+) ([0] ++ us ++ [0]) $",
				"                      zipWith (+) ([0] ++ vs) (vs ++ [0]))) ([1], [1, 2])",
				"-- _Reinhard Zumkeller_, Jul 20 2013",
				"(Sage)",
				"def A035607_row(n):",
				"    @cached_function",
				"    def prec(n, k):",
				"        if k==n: return 1",
				"        if k==0: return 0",
				"        return prec(n-1,k-1)+2*sum(prec(n-i,k-1) for i in (2..n-k+1))",
				"    return [prec(n, n-k) for k in (0..n-1)]",
				"for n in (1..10): print A035607_row(n) # _Peter Luschny_, Mar 16 2016",
				"(PARI) T(n, k) = if (k==0, 1, sum(i=0, k-1, binomial(n-k,i+1)*binomial(k-1,i)*2^(i+1)));",
				"tabl(nn) = for (n=1, nn, for (k=0, n-1, print1(T(n, k), \", \")); print); \\\\ as a triangle; _Michel Marcus_, Feb 27 2018"
			],
