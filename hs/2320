			"program": [
				"(PARI)",
				"F=vector(30,n,n^3); /* modify to get other sequences of \"greedy representations\" */ last_leq(v,F)=",
				"{ /* Return last element \u003c=v in sorted array F[] */",
				"    local(j=1);",
				"    while ( F[j]\u003c=v, j+=1 );",
				"    return( F[j-1] );",
				"}",
				"greedy(n,F)=",
				"{",
				"    local(v=n,ct=0);",
				"    while ( v,  v-=last_leq(v,F); ct+=1; );",
				"    return(ct);",
