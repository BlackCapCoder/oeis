				"    B=a(n-1)-(n-1);",
				"    C=0.^( abs(B+1) + (B+1) );",
				"    D=ismember(B,a(1:n-1));",
				"    a(n)=a(n-1)+ (n-1) * (-1)^(C + D -1);",
				"end",
