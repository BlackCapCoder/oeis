			"maple": [
				"wieferich := proc (n) local nsq, remain, bin, char: if (not isprime(n)) then RETURN(\"not prime\") fi: nsq := n^2: remain := 2: bin := convert(convert(n-1, binary),string): remain := (remain * 2) mod nsq: bin := substring(bin,2..length(bin)): while (length(bin) \u003e 1) do: char := substring(bin,1..1): if char = \"1\" then remain := (remain * 2) mod nsq fi: remain := (remain^2) mod nsq: bin := substring(bin,2..length(bin)): od: if (bin = \"1\") then remain := (remain * 2) mod nsq fi: if remain = 1 then RETURN (\"Wieferich prime\") fi: RETURN (\"non-Wieferich prime\"): end: # Ulrich Schimke (ulrschimke(AT)aol.com), Nov 01 2001"
