				"    def say(s):",
				"        acc = '' # initialize accumulator",
				"        while len(s) \u003e 0:",
				"            i = 0",
				"            c = s[0] # char of first run",
				"            while (i \u003c len(s) and s[i] == c): # scan first digit run",
				"                i += 1",
				"            acc += str(i) + c # append description of first run",
				"            if i == len(s):",
				"                break # done",
				"            else:",
				"                s = s[i:] # trim leading run of digits",
				"        return acc",
				"    for i in xrange(1, n):",
				"        seq[i] = int(say(str(seq[i-1])))",
