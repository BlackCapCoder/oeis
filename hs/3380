				"(Haskell)",
				"a007061 n = a007061_list !! (n-1)",
				"a007061_list = 1 : f [1] where",
				"   f us = a' : f (us ++ [a']) where",
				"     a' = b $ reverse $ map (`splitAt` us) [0..length us - 1] where",
				"        b ((xs,ys):xyss) | vs `isSuffixOf` xs = 3 - head ys",
				"                         | otherwise          = b xyss",
				"     vs = fromJust $ find (`isInfixOf` init us) $ tails us",
