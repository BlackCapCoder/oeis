				"def A038207_triangle(dim):",
				"    M = matrix(SR,dim,dim)",
				"    for n in range(dim): M[n,n] = 1",
				"    for n in (1..dim-1):",
				"        for k in (0..n-1):",
				"            M[n,k] = M[n-1,k-1]+2*M[n-1,k]",
				"    return M",
