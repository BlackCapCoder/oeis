				"(Haskell)",
				"a038207 n = a038207_list !! n",
				"a038207_list = concat $ iterate ([2,1] *) [1]",
				"instance Num a =\u003e Num [a] where",
				"   fromInteger k = [fromInteger k]",
				"   (p:ps) + (q:qs) = p + q : ps + qs",
				"   ps + qs         = ps ++ qs",
				"   (p:ps) * qs'@(q:qs) = p * q : ps * qs' + [p] * qs",
