				"function isMU(u, n, h, i, r)",
				"    ur = u[r]; ui = u[i]",
				"    ur \u003c= ui \u0026\u0026 return h",
				"    if ur * ui \u003e n",
				"        r -= 1",
				"    elseif ur * ui \u003c n",
				"        i += 1",
				"    else",
				"        h \u0026\u0026 return false",
				"        h = true; i += 1; r -= 1",
				"    end",
				"    isMU(u, n, h, i, r)",
