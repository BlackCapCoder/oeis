				"TableForm[cu]",
				"Flatten[%]  (* A208510 *)",
				"Table[Expand[v[n, x]], {n, 1, z}]",
				"cv = Table[CoefficientList[v[n, x], x], {n, 1, z}];",
				"TableForm[cv]",
				"Flatten[%]  (* A029653 *)",
				"(* _Clark Kimberling_, Feb 28 2012 *)"
			],
			"program": [
				"(Haskell)",
				"a029653 n k = a029653_tabl !! n !! k",
				"a029653_row n = a029653_tabl !! n",
				"a029653_tabl = [1] : iterate",
				"               (\\xs -\u003e zipWith (+) ([0] ++ xs) (xs ++ [0])) [2, 1]",
				"-- _Reinhard Zumkeller_, Dec 16 2013",
				"(Python)",
				"from sympy import Poly",
				"def u(n, x): return 1 if n==1 else u(n - 1, x) + x*v(n - 1, x)",
				"def v(n, x): return 1 if n==1 else u(n - 1, x) + x*v(n - 1, x) + 1",
				"def a(n): return Poly(v(n, x), x).all_coeffs()[::-1]",
				"for n in xrange(1, 13): print a(n) # _Indranil Ghosh_, May 27 2017"
