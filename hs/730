				"    for d in numtheory[divisors](n) do",
				"        if d mod 4 = 1 then",
				"            count1 := count1+1",
				"        elif d mod 4 = 3 then",
				"            count3 := count3+1",
				"        fi:",
				"    end do:",
				"    count1-count3;",
				"end proc:"
