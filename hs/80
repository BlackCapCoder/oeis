			"maple": [
				"with(numtheory):",
				"T := proc(n::integer, m::integer)",
				"        local A, summe, d:",
				"        if isprime(n) then",
				"                if n \u003c= m then",
				"                        return 1;",
				"                end if:",
				"                return 0 ;",
				"        end if:",
				"        A := divisors(n) minus {n, 1}:",
				"        for d in A do",
				"                if d \u003e m then",
				"                        A := A minus {d}:",
				"                end if:",
				"        end do:",
				"        summe := add(T(n/d,d),d=A) ;",
				"        if n \u003c=m then",
				"                summe := summe + 1:",
				"        end if:",
				"        summe ;",
				"end proc:",
