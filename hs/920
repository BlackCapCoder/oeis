				"  M:=200; traj:=[n]; h:=n; s:=1;",
				"  for i from 1 to M do j:=h-s*i;",
				"    if member(j,traj,'p') then s:=-s; fi;",
				"    h:=h-s*i; traj:=[op(traj),h];",
