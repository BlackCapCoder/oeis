#!/bin/bash


printf 'r=read::String->Int;main = interact $ show . length . map r . lines' > /tmp/tmp.hs
# printf 'r=read::String->Int;main = interact $ length . map r . lines' > /tmp/tmp.hs
# printf 'r=read::String->Int;main = interact $ unwords . lines ' > /tmp/tmp.hs

cat src/OEIS/Part1.hs \
    src/OEIS/Part2.hs \
    src/OEIS/Prime.hs \
    src/OEIS/Simple.hs \
  | grep -Po '^instance OEIS \K\d*' | sort -n | runhaskell /tmp/tmp.hs
